  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "gh-pages": "^2.1.1",
    "lodash": "^4.17.15",
    "node-sass": "^4.12.0",
    "np": "^5.1.0",
    "onchange": "^6.1.0",
    "prettier": "^1.18.2",
    "prettier-webpack-plugin": "^1.2.0",
    "react-app-rewired": "^2.1.3"
  }
